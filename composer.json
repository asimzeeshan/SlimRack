{
    "name": "asimzeeshan/slimrack",
    "description": "A lightweight VPS and server inventory management system built with Slim Framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Asim Zeeshan",
            "email": "",
            "homepage": "https://www.linkedin.com/in/asimzeeshan"
        }
    ],
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SlimRack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlimRack\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
